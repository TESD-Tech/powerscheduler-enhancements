~[tlist_sql;
SELECT JSON_ARRAYAGG(
		JSON_OBJECT( * RETURNING CLOB)
    RETURNING CLOB
	) json_obj
FROM (
SELECT SCHEDULECOURSECATALOGS.DCID
	, SCHEDULECOURSECATALOGS.COURSE_NUMBER
	, SCHEDULECOURSECATALOGS.SCHED_FULLCATALOGDESCRIPTION
FROM SCHEDULECOURSECATALOGS
WHERE schoolid = ~(curschoolid)
	AND COURSECATALOGID = ~[x:scheduleinfo;curcatalogid]
) courses
;nonemessage=[]]~(json_obj)[/tlist_sql]